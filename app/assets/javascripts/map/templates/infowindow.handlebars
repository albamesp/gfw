          <h2>{{content.data.name}}</h2>
        {{/if}}

        {{!-- <div class="thumb-mask" style="background-image: url({{content.data.image}})"> --}}
          <div class="thumb-mask"><img src="{{content.data.image}}" onerror="this.style.display='none';"></div>

      {{/if}}
    </div>
    {{#if content.data.analysis}}
    <div id="cartodb-popup-fixed" class="cartodb-popup-fixed">
      {{#if content.data.analysis}}
        <a style="cursor:pointer;" id="analyzeBtn" class="analyze-concession btn green uppercase" data-layer="{{content.data.id}}" data-useid="{{content.data.cartodb_id}}" data-use="{{content.data.tablename}}" data-wdpaid="{{content.data.id}}">analyze</a>
      {{/if}}
        <a style="cursor:pointer;" id="subscriptionBtn" class="subscription-concession btn green uppercase" data-layer="{{content.data.id}}" data-useid="{{content.data.cartodb_id}}" data-use="{{content.data.tablename}}" data-wdpaid="{{content.data.id}}">subscribe</a>
    </div>
    {{/if}}
